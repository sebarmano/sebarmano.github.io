@charset "utf-8";

 <%= Rouge::Themes::Github.render(:scope => '.highlight') %>

 .gutter {
   width: 30px;
   background: #eee;
   color: #b1b1b1;
 }

 .highlight {
   font-size: 1.2em;
 }

 .highlight table td.code {
